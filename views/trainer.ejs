  <body>
    <div class="main-container">
      <div class="header flex">
        <img src="./img-trainer/logo.png" alt="" />
        <a class="nav" href="">Trainers</a>
        <a class="nav trainer" href="">Activities</a>
        <img src="./img-trainer/user-profile-img.jpg" alt="" />
      </div>
      <hr class="orange-line" />
      <div class="training-type flex">
        <div class="training-img">
          <img src="./img-trainer/obedience.png" alt="" />
          <p>Obedience</p>
        </div>
        <div class="training-img">
          <img src="./img-trainer/protection.png" alt="" />
          <p>Protection</p>
        </div>
