        </div>
        <div class="training-img">
          <img src="./img-trainer/others.png" alt="" />
          <p>Others</p>
        </div>
      </div>
      <div class="search-bar-div">
        <input
          type="text"
          class="search-bar"
          placeholder="Search for a trainer"
        />
      </div>
      <!-- trainer profile -->
